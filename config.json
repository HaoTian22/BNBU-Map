{
  "options": {
    "paths": {
      "root": "",
      "styles": "styles",
      "fonts": "fonts",
      "mbtiles": "data"
    },
    "domains": [
      "vps.haotian22.top:8081"
    ],
    "formatOptions": {
      "jpeg": {
        "quality": 80
      },
      "webp": {
        "quality": 90
      }
    },
    "maxScaleFactor": 3,
    "maxSize": 2048,
    "pbfAlias": "pbf",
    "serveAllFonts": false,
    "serveAllStyles": false,
    "serveStaticMaps": true,
    "allowRemoteMarkerIcons": true,
    "allowInlineMarkerImages": true,
    "staticAttributionText": "© OpenMapTiles  © OpenStreetMaps",
    "tileMargin": 0
  },
  "styles": {
    "osm-liberty": {
      "style": "osm_liberty.json",
      "serve_rendered": true
    },
    "remote": {
      "style": "https://demotiles.maplibre.org/style.json"
    }
  },
  "data": {
    "openmaptiles": {
      "mbtiles": "BNBU.mbtiles"
    }
  }
}